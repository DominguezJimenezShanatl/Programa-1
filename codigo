void setup() {
  // Inicio de variables
  pinMode(LED_BUILTIN, OUTPUT);
}

// the loop function runs over and over again forever
void loop() {    
  //loop es una funcion repetitiva, se mantiene en bucle
  digitalWrite(LED_BUILTIN, HIGH);   // Manda una se√±al al dispoitivo, prende el led
  delay(1000);                       // Tiempo de espera, se mantiene encendido el led
  digitalWrite(LED_BUILTIN, LOW);    // apaga el led
  delay(1000);                       // espera un segundo
}
